{
  "roots": [
    "<rootDir>/packages/node_modules/"
  ],
  "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
  "transformIgnorePatterns": ["<rootDir>/node_modules/(?!@webex/sdk-component-adapter)"],
  "moduleNameMapper": {
    "^.+\\.(css|scss|less)$": "identity-obj-proxy",
    "^.+\\.(mp3|wav)$": "<rootDir>/__mocks__/sound-mock.js",
    "^.+\\.(gif|ttf|eot|svg|png|jpg)$": "<rootDir>/__mocks__/file-mock.js",
    "@ciscospark/react-(.*)": "<rootDir>/packages/node_modules/@ciscospark/react-$1/src",
    "@ciscospark/redux-(.*)": "<rootDir>/packages/node_modules/@ciscospark/redux-$1/src",
    "@ciscospark/widget-(.*)": "<rootDir>/packages/node_modules/@ciscospark/widget-$1/src",
    "@ciscospark/spark-widget-base": "<rootDir>/packages/node_modules/@ciscospark/spark-widget-base/src",
    "@webex/react-(.*)": "<rootDir>/packages/node_modules/@webex/react-$1/src",
    "@webex/redux-(.*)": "<rootDir>/packages/node_modules/@webex/redux-$1/src",
    "@webex/widget-(.*)": "<rootDir>/packages/node_modules/@webex/widget-$1/src",
    "@webex/components": "<rootDir>/node_modules/@webex/components/dist/umd/webex-components.umd.js",
    "@webex/webex-widget-base": "<rootDir>/packages/node_modules/@webex/webex-widget-base/src"
  },
  "testRegex": ".test.js$",
  "testURL": "http://localhost",
  "moduleDirectories": [
    "node_modules",
    "/packages/node_modules/"
  ],
  "modulePaths": [
    "<rootDir>/node_modules",
    "<rootDir>/node_modules/@ciscospark",
    "<rootDir>/node_modules/@webex",
    "<rootDir>/packages/node_modules"
  ],
  "haste": {
    "providesModuleNodeModules": [".*"]
  },
  "verbose": true,
  "setupFiles": [
    "<rootDir>/scripts/tests/jest-setup.js"
  ]
}
